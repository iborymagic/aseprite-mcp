{
  "name": "@iborymagic/aseprite-mcp",
  "version": "0.4.9",
  "description": "MCP server for using Aseprite API",
  "type": "module",
  "bin": {
    "aseprite-mcp": "build/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run copy:lua",
    "copy:lua": "rm -rf build/lua/templates && cp -R src/lua/templates build/lua/templates",
    "start": "node build/index.js",
    "dev": "npm run build && npm run start",
    "test": "vitest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "axios": "^1.13.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "author": "iborymagic",
  "repository": {
    "type": "git",
    "url": "https://github.com/iborymagic/aseprite-mcp"
  },
  "bugs": {
    "url": "https://github.com/iborymagic/aseprite-mcp/issues"
  },
  "keywords": [
    "mcp",
    "aseprite"
  ],
  "license": "MIT"
}
